pipeline{
    agent any
    stages {
        stage('maven编译'){
            steps{
                //mvn构建
                sh "mvn clean install"
            }
        }
        
        stage('docker镜像创建'){
            steps{
                sh 'docker-compose build --pull'
                sh 'docker-compose push'
            }
        }
        
        stage('环境构建'){
            steps{
                sh 'docker-compose up'
            }
        }
    }
}
